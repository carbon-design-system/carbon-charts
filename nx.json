{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "metadata",
          "styles",
          "build:package",
          "mdx",
          "build:demo",
          "build",
          "bundle:package",
          "styles:scss:charts",
          "styles:css:charts",
          "styles:css:min:charts",
          "styles:charts",
          "styles:demo",
          "build:docs",
          "license"
        ]
      }
    }
  },
  "targetDefaults": {
    "build:package": {
      "dependsOn": [
        "^build:package"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:demo": {
      "dependsOn": [
        "^build:demo"
      ],
      "outputs": [
        "{projectRoot}/demo/bundle"
      ]
    },
    "metadata": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "mdx": {
      "outputs": [
        "{projectRoot}/src/stories"
      ]
    },
    "bundle:package": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles:scss:charts": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles:css:charts": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles:css:min:charts": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles:charts": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "styles:demo": {
      "outputs": [
        "{projectRoot}/dist/demo"
      ]
    },
    "build:docs": {
      "outputs": [
        "{projectRoot}/demo/bundle/documentation"
      ]
    }
  }
}
