{
	"name": "@carbon/charts-react",
	"version": "1.6.14",
	"description": "Carbon charting components for React",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"browser": {
				"import": "./dist/index.mjs",
				"require": "./dist/index.js"
			},
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build:package": "vite build",
		"build:demo": "storybook build -o demo/bundle --quiet --disable-telemetry",
		"build": "yarn build:package && yarn build:demo",
		"storybook": "storybook dev -p 9006",
		"start": "yarn storybook",
		"ci-check": "yarn format:diff",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier . --write \"**/*.{scss,css,js,ts,tsx,md}\"",
		"format:diff": "prettier --list-different src/",
		"clean": "rimraf dist demo/bundle"
	},
	"dependencies": {
		"@carbon/charts": "workspace:^1.6.14",
		"@carbon/telemetry": "~0.1.0",
		"d3": "^7.8.3"
	},
	"peerDependencies": {
		"d3": "^7.8.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@carbon/icons-react": "^11.17.0",
		"@rushstack/eslint-patch": "^1.2.0",
		"@storybook/addon-essentials": "^7.0.0-0",
		"@storybook/addon-interactions": "^7.0.0-0",
		"@storybook/addon-links": "^7.0.0-0",
		"@storybook/blocks": "^7.0.0-0",
		"@storybook/react": "^7.0.0-0",
		"@storybook/react-vite": "^7.0.0-0",
		"@storybook/testing-library": "^0.0.14-next.1",
		"@types/carbon__icons-react": "^11.16.0",
		"@types/react": "^18.0.29",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vitejs/plugin-react": "^3.1.0",
		"classnames": "^2.3.2",
		"codesandbox": "^2.2.3",
		"eslint": "8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.7",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rimraf": "^4.4.1",
		"sass": "^1.60.0",
		"storybook": "^7.0.0-0",
		"typescript": "~4.9.5",
		"vite": "^4.2.1"
	},
	"prettier": "../../prettier.config.mjs",
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:carbon-design-system/carbon-charts.git",
		"homepage": "https://carbon-design-system.github.io/carbon-charts/react/"
	},
	"bugs": {
		"url": "https://github.com/carbon-design-system/carbon-charts/issues"
	},
	"homepage": "https://github.com/carbon-design-system/carbon-charts#readme",
	"maintainers": [
		{
			"name": "Eliad Moosavi",
			"email": "iliadm@ca.ibm.com",
			"url": "https://github.com/theiliad"
		}
	],
	"contributors": [
		{
			"name": "Eliad Moosavi",
			"email": "iliadm@ca.ibm.com",
			"url": "https://github.com/theiliad"
		}
	],
	"license": "Apache-2.0",
	"author": "IBM",
	"keywords": [
		"carbon",
		"charts",
		"react",
		"dataviz",
		"data-visualization",
		"visualizations",
		"d3",
		"svg",
		"component",
		"components",
		"css",
		"html",
		"ibm",
		"typescript",
		"javascript",
		"js",
		"library",
		"pattern",
		"patterns",
		"sass",
		"scss"
	]
}
