{
  "name": "@carbon/charts-angular",
  "version": "1.7.6",
  "description": "Carbon charting components for Angular",
  "module": "./dist/index.mjs",
  "types": "./dist/main.d.ts",
  "typings": "./dist/main.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "scripts": {
    "postinstall": "carbon-telemetry collect --install",
    "license": "cp ../../LICENSE.md ./",
    "styles": "cp ../core/dist/styles.* dist",
    "build:package:new": "vite build && yarn license && yarn styles",
    "build:package": "ng build && yarn styles && yarn license",
    "build:demo:new": "storybook build -o demo/bundle --quiet",
    "build:demo": "ng run charts-angular:build-storybook",
    "build": "yarn build:package && yarn build:demo",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --fix --ignore-path ../../.gitignore",
    "storybook:new": "storybook dev -p 9006",
    "storybook": "ng run charts-angular:storybook",
    "start": "yarn storybook",
    "format": "prettier --write src/ --ignore-path ../../.gitignore",
    "clean": "rm -rf dist demo/bundle"
  },
  "peerDependencies": {
    "@angular/common": "*",
    "@angular/core": "*"
  },
  "dependencies": {
    "@angular/common": "16.0.0-rc.4",
    "@angular/core": "16.0.0-rc.4",
    "@carbon/charts": "workspace:*",
    "@carbon/icons-angular": "^11.2.2",
    "@carbon/telemetry": "~0.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.0-rc.4",
    "@angular/animations": "16.0.0-rc.4",
    "@angular/cli": "16.0.0-rc.4",
    "@angular/compiler": "16.0.0-rc.4",
    "@angular/compiler-cli": "16.0.0-rc.4",
    "@angular/forms": "16.0.0-rc.4",
    "@angular/platform-browser": "16.0.0-rc.4",
    "@babel/core": "^7.21.8",
    "@compodoc/compodoc": "^1.1.19",
    "@stackblitz/sdk": "^1.9.0",
    "@storybook/addon-essentials": "^7.1.0-0",
    "@storybook/addon-interactions": "^7.1.0-0",
    "@storybook/addon-links": "^7.1.0-0",
    "@storybook/angular": "^7.1.0-0",
    "@storybook/blocks": "^7.1.0-0",
    "@storybook/manager-api": "^7.1.0-0",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.1.0-0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "ng-packagr": "16.0.0-rc.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "storybook": "^7.1.0-0",
    "style-loader": "3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-dts": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:carbon-design-system/carbon-charts.git",
    "homepage": "https://carbon-design-system.github.io/carbon-charts/angular/"
  },
  "license": "Apache-2.0",
  "author": "IBM",
  "keywords": [
    "carbon",
    "charts",
    "angular",
    "dataviz",
    "data-visualization",
    "visualizations",
    "d3",
    "svg",
    "component",
    "components",
    "css",
    "html",
    "ibm",
    "typescript",
    "javascript",
    "js",
    "library",
    "pattern",
    "patterns",
    "sass",
    "scss"
  ],
  "maintainers": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ],
  "contributors": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ]
}
