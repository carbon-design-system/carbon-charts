{
	"name": "@carbon/charts-vue",
	"version": "1.16.0",
	"description": "Carbon Charts component library for Vue",
	"type": "module",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./styles.min.css": "./dist/styles.min.css",
		"./styles.css": "./dist/styles.css",
		".": {
			"style": "./dist/styles.css",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"dist",
		"CHANGELOG.md",
		"telemetry.yml"
	],
	"scripts": {
		"postinstall": "ibmtelemetry --config=telemetry.yml",
		"file:styles": "cp ../core/dist/styles.* dist",
		"build": "vite build && yarn file:styles",
		"build:test": "vite build --config vite.test.config.ts --emptyOutDir",
		"dev": "vite",
		"lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --fix --ignore-path ../../.eslintignore",
		"format": "prettier --write src/ --ignore-path ../../.eslintignore"
	},
	"peerDependencies": {
		"vue": "^3.4.0"
	},
	"dependencies": {
		"@carbon/charts": "workspace:*",
		"@ibm/telemetry-js": "^1.5.1",
		"vue": "^3.4.27"
	},
	"devDependencies": {
		"@types/d3": "^7.4.3",
		"@vitejs/plugin-vue": "^5.0.4",
		"d3": "^7.9.0",
		"d3-cloud": "^1.2.7",
		"d3-sankey": "^0.12.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-dts": "^3.9.1",
		"vue-tsc": "^2.0.18"
	},
	"publishConfig": {
		"access": "public",
		"tag": "next"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:carbon-design-system/carbon-charts.git",
		"homepage": "https://charts.carbondesignsystem.com/vue/"
	},
	"license": "Apache-2.0",
	"author": "IBM",
	"keywords": [
		"charts",
		"graphs",
		"radar",
		"gauge",
		"donut",
		"pie",
		"sparkline",
		"tree",
		"treemap",
		"heatmap",
		"wordcloud",
		"histogram",
		"alluvial",
		"geo",
		"bar",
		"bullet",
		"scatter",
		"meter",
		"line",
		"vue",
		"vue3",
		"component",
		"carbon",
		"ibm",
		"svg",
		"data",
		"typescript"
	]
}
