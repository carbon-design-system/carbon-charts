{
	"name": "@carbon/charts",
	"version": "1.6.14",
	"description": "Carbon charting components",
	"module": "./dist/index.mjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts",
			"browser": {
				"import": "./dist/index.mjs",
				"require": "./dist/index.js"
			}
		},
		"./package.json": "./package.json",
		"./styles.min.css": "./dist/styles.min.css",
		"./styles.css": "./dist/styles.css",
		"./demo/styles.css": "./dist/demo/styles.css",
		"./demo": {
			"import": "./dist/demo/index.mjs",
			"require": "./dist/demo/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"bundle:package": "vite build",
		"styles:scss:charts": "cp -a src/styles/. dist/styles",
		"styles:css:charts": "sass src/styles/styles.scss dist/styles.css --load-path=./node_modules",
		"styles:css:min:charts": "sass src/styles/styles.scss dist/styles.min.css --style=compressed --load-path=./node_modules",
		"styles:charts": "yarn styles:scss:charts && yarn styles:css:charts && yarn styles:css:min:charts",
		"md:charts": "cp CHANGELOG.md dist/ && cp ../../LICENSE.md dist/",
		"bundle:demo:data": "vite build --config ./vite.demo.config.ts",
		"styles:demo": "sass demo/styles.scss dist/demo/styles.css --load-path=./node_modules",
		"build:docs": "typedoc --tsconfig ./src/tsconfig.json --out ./demo/bundle/documentation ./src/index.ts",
		"build:package": "yarn bundle:package && yarn bundle:demo:data && yarn styles:charts && yarn styles:demo && yarn build:docs && yarn md:charts",
		"build:demo": "storybook build -o demo/bundle --quiet",
		"build": "yarn build:package && yarn build:demo",
		"storybook": "storybook dev -p 9006",
		"start": "yarn storybook",
		"test": "karma start --single-run",
		"test:watch": "karma start --no-single-run",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
		"ci-check": "yarn format:diff",
		"format": "prettier . --write \"**/*.{scss,css,js,ts,md}\"",
		"format:diff": "prettier --list-different src/",
		"clean": "rm -rf dist demo/bundle"
	},
	"peerDependencies": {
		"d3": "^7.8.4"
	},
	"dependencies": {
		"@carbon/styles": "^1.27.0",
		"@carbon/telemetry": "~0.1.0",
		"@carbon/utils-position": "^1.1.4",
		"@types/node": "^18.15.11",
		"carbon-components": "^10.58.6",
		"d3-array": "^3.2.3",
		"d3-axis": "^3.0.0",
		"d3-brush": "^3.0.0",
		"d3-cloud": "^1.2.5",
		"d3-color": "^3.1.0",
		"d3-ease": "^3.0.1",
		"d3-hierarchy": "^3.1.2",
		"d3-interpolate": "^3.0.1",
		"d3-path": "^3.1.0",
		"d3-sankey": "^0.12.3",
		"d3-scale": "^4.0.2",
		"d3-selection": "^3.0.0",
		"d3-shape": "^3.2.0",
		"d3-time-format": "^4.1.0",
		"d3-transition": "^3.0.1",
		"date-fns": "^2.29.3",
		"dom-to-image-more": "^3.1.1",
		"lodash-es": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"resize-observer-polyfill": "^1.5.1",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@carbon/colors": "^11.14.0",
		"@carbon/elements": "^11.21.0",
		"@carbon/grid": "^11.13.0",
		"@carbon/import-once": "^10.7.0",
		"@carbon/layout": "^11.13.0",
		"@carbon/motion": "^11.10.0",
		"@carbon/themes": "^11.18.0",
		"@carbon/type": "^11.17.0",
		"@rushstack/eslint-patch": "^1.2.0",
		"@stackblitz/sdk": "^1.9.0",
		"@storybook/addon-essentials": "^7.0.5",
		"@storybook/addon-interactions": "^7.0.5",
		"@storybook/addon-links": "^7.0.5",
