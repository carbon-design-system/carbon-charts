		"@storybook/theming": "^7.0.5",
		"@types/d3": "^7.4.0",
		"@types/jasmine": "^4.3.1",
		"@types/karma": "^6.3.3",
		"@types/marked": "^4.0.8",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"codesandbox": "^2.2.3",
		"dom-to-image-more": "^3.1.1",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.11",
		"highlight.js": "^11.7.0",
		"jasmine-core": "^4.6.0",
		"karma": "^6.4.1",
		"karma-chrome-launcher": "^3.1.1",
		"karma-firefox-launcher": "^2.1.2",
		"karma-jasmine": "^5.1.0",
		"karma-safari-launcher": "^1.0.0",
		"marked": "^4.3.0",
		"prettier": "^2.8.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.62.0",
		"storybook": "^7.0.5",
		"typedoc": "^0.24.2",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vite-plugin-dts": "^2.2.0"
	},
	"prettier": "../../prettier.config.mjs",
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
