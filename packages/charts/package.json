{
  "name": "@carbon/charts",
  "version": "1.6.14",
  "description": "Carbon charting components",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "./package.json": "./package.json",
    "./styles.min.css": "./dist/styles.min.css",
    "./styles.css": "./dist/styles.css",
    "./demo/styles.css": "./dist/demo/styles.css",
    "./demo": {
      "import": "./dist/demo/index.mjs",
      "require": "./dist/demo/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts"
    },
    "./charts": {
      "types": "./dist/charts/index.d.ts"
    },
    "./components": {
      "types": "./dist/components/index.d.ts"
    },
    "./services": {
      "types": "./dist/services/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "bundle:package": "vite build",
    "styles:scss:charts": "cp -a src/styles/. dist/styles",
    "styles:css:charts": "sass --load-path ../../node_modules src/styles/styles.scss dist/styles.css",
    "styles:css:min:charts": "sass --load-path ../../node_modules src/styles/styles.scss dist/styles.min.css --style=compressed",
    "styles:charts": "yarn styles:scss:charts && yarn styles:css:charts && yarn styles:css:min:charts",
    "md:charts": "cp CHANGELOG.md dist/ && cp ../../LICENSE.md dist/",
    "build:package": "yarn bundle:package && yarn styles:charts && yarn md:charts",
    "bundle:demo:data": "vite build --config ./vite.demo.config.ts",
    "styles:demo": "sass --load-path ../../node_modules  demo/styles.scss dist/demo/styles.css",
    "bundle:storybook": "storybook build -o demo/bundle --quiet",
    "build:docs": "typedoc --tsconfig ./src/tsconfig.json --out ./demo/bundle/documentation ./src/index.ts",
    "build:demo": "yarn bundle:demo:data && yarn styles:demo && yarn bundle:storybook && yarn build:docs",
    "build": "yarn build:package && yarn build:demo",
    "storybook": "storybook dev -p 9006",
    "start": "yarn storybook",
    "test": "karma start --single-run",
    "test:watch": "karma start --no-single-run",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "ci-check": "yarn format:diff",
    "format": "prettier . --write \"**/*.{scss,css,js,ts,md}\"",
    "format:diff": "prettier --list-different src/",
    "clean": "rimraf dist demo/bundle"
  },
  "peerDependencies": {
    "d3": "^7.4.5"
  },
  "dependencies": {
    "@carbon/styles": "^1.24.0",
    "@carbon/telemetry": "~0.1.0",
    "@carbon/utils-position": "^1.1.4",
    "@types/node": "^18.15.5",
    "carbon-components": "^10.58.3",
    "d3-array": "^3.2.2",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-cloud": "^1.2.5",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.29.3",
    "dom-to-image-more": "^3.1.0",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@carbon/colors": "^11.2.0",
    "@carbon/elements": "^11.3.0",
    "@carbon/grid": "^11.2.0",
    "@carbon/import-once": "^10.7.0",
    "@carbon/layout": "^11.2.0",
    "@carbon/motion": "^11.1.0",
    "@carbon/themes": "^11.3.0",
    "@carbon/type": "^11.3.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-docs": "^7.0.0-0",
    "@storybook/addon-essentials": "^7.0.0-0",
    "@storybook/addon-links": "^7.0.0-0",
    "@storybook/blocks": "^7.0.0-0",
    "@storybook/html": "^7.0.0-0",
    "@storybook/html-vite": "^7.0.0-0",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-0",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "^4.3.1",
    "@types/karma": "^6.3.3",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "codesandbox": "^2.2.3",
    "dom-to-image": "2.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-storybook": "^0.6.11",
    "highlight.js": "^11.7.0",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-safari-launcher": "^1.0.0",
    "lerna": "^6.5.1",
    "marked": "^4.2.12",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0",
    "sass": "^1.59.2",
    "storybook": "^7.0.0-0",
    "typedoc": "^0.23.27",
    "typescript": "~4.8.4",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:carbon-design-system/carbon-charts.git",
    "homepage": "https://carbon-design-system.github.io/carbon-charts/"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-charts/issues"
  },
  "homepage": "https://github.com/carbon-design-system/carbon-charts#readme",
  "maintainers": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ],
  "contributors": [
    {
      "name": "Eliad Moosavi",
