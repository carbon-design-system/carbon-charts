    }
  ],
  "contributors": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ],
  "gitHead": "f7037103474e53901678dd9e62095b0b1f0f62c4"
}
