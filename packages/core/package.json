{
  "name": "@carbon/charts",
  "version": "1.7.6",
  "description": "Carbon charting components",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./styles.min.css": "./dist/styles.min.css",
    "./styles.css": "./dist/styles.css",
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./demo/styles.css": "./dist/demo/styles.css",
    "./demo": {
      "types": "./dist/demo/index.d.ts",
      "import": "./dist/demo/index.mjs",
      "require": "./dist/demo/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.mjs",
      "require": "./dist/interfaces/index.js"
    },
    "./model": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.mjs",
      "require": "./dist/model/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "scripts": {
    "postinstall": "carbon-telemetry collect --install",
    "bundle:package": "vite build",
    "styles:scss:charts": "cp -a src/styles/. dist/styles",
    "styles:css:charts": "sass src/styles/styles.scss dist/styles.css --load-path=./node_modules",
    "styles:css:min:charts": "sass src/styles/styles.scss dist/styles.min.css --style=compressed --load-path=./node_modules",
    "styles:charts": "yarn styles:scss:charts && yarn styles:css:charts && yarn styles:css:min:charts",
    "styles:demo": "sass src/demo/styles.scss dist/demo/styles.css --load-path=./node_modules",
    "build:docs": "typedoc ./src/index.ts --out ./demo/bundle/documentation",
    "license": "cp ../../LICENSE.md ./",
    "build:package": "yarn bundle:package && yarn styles:charts && yarn styles:demo && yarn build:docs && yarn license",
    "build:demo": "storybook build -o demo/bundle --quiet",
    "build": "yarn build:package && yarn build:demo",
    "storybook": "storybook dev -p 9006",
    "start": "yarn storybook",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts --fix --ignore-path ../../.eslintignore",
    "format": "prettier . --write \"**/*.{scss,css,js,ts,mjs,cjs,md}\" --ignore-path ../../.eslintignore",
    "clean": "rm -rf dist demo/bundle"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:carbon-design-system/carbon-charts.git",
    "homepage": "https://carbon-design-system.github.io/carbon-charts/"
  },
  "dependencies": {
    "@carbon/colors": "^11.14.0",
    "@carbon/import-once": "^10.7.0",
    "@carbon/layout": "^11.13.0",
    "@carbon/styles": "^1.28.0",
    "@carbon/telemetry": "~0.1.0",
    "@carbon/themes": "^11.18.0",
    "@carbon/utils-position": "^1.1.4",
    "carbon-components": "^10.58.6",
    "d3-array": "^3.2.3",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-cloud": "^1.2.5",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.30.0",
    "dom-to-image-more": "^3.1.6",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "tslib": "^2.5.0"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@stackblitz/sdk": "^1.9.0",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/blocks": "^7.0.8",
    "@storybook/html": "^7.0.8",
    "@storybook/html-vite": "^7.0.8",
    "@storybook/manager-api": "^7.0.8",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.8",
    "@types/d3": "^7.4.0",
    "@types/lodash-es": "4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "codesandbox": "^2.2.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "^22.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "storybook": "^7.0.8",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-dts": "~2.3.0",
    "vitest": "0.31.0"
  },
  "prettier": "../../prettier.config.cjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "carbon",
    "charts",
    "dataviz",
    "data-visualization",
    "visualizations",
    "d3",
    "svg",
    "component",
    "components",
    "css",
    "html",
    "ibm",
    "typescript",
    "javascript",
    "js",
    "library",
    "pattern",
    "patterns",
    "sass",
    "scss"
  ],
  "author": "IBM",
  "license": "Apache-2.0",
  "maintainers": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ],
  "contributors": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ]
}
