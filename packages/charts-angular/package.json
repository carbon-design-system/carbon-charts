{
  "name": "@carbon/charts-angular",
  "version": "1.6.14",
  "description": "Workspace for building Carbon charting components for Angular",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ng": "ng",
    "copy:md": "cp CHANGELOG.md dist/ && cp ../../LICENSE.md dist/",
    "build:package": "ng build",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && ng run charts-angular:storybook",
    "start": "yarn storybook",
    "build:demo": "yarn docs:json && ng run charts-angular:build-storybook",
    "build": "yarn build:package && yarn copy:md && yarn build:demo",
    "clean": "rimraf dist demo/bundle",
    "lint": "ng lint",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write projects/charts-angular/src/",
    "format:diff": "prettier --list-different projects/charts-angular/src/",
    "ci-check": "yarn format:diff"
  },
  "peerDependencies": {
    "@angular/common": "^14.2.0 || ^15.0.0 || ^16.0.0",
    "@angular/core": "^14.2.0 || ^15.0.0 || ^16.0.0",
