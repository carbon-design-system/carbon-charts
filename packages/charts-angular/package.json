{
  "name": "@carbon/charts-angular",
  "version": "1.6.14",
  "description": "Workspace for building Carbon charting components for Angular",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.min.css": "./dist/styles.min.css",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ng": "ng",
    "copy:md": "cp CHANGELOG.md dist/ && cp ../../LICENSE.md dist/",
    "cp:styles": "cp ../charts/dist/styles.* dist",
    "build:package": "ng build && yarn cp:styles",
    "storybook": "ng run charts-angular:storybook",
    "start": "yarn storybook",
    "build:demo": "ng run charts-angular:build-storybook",
    "build": "yarn build:package && yarn copy:md && yarn build:demo",
    "clean": "rm -rf dist demo/bundle",
    "lint": "ng lint",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write projects/charts-angular/src/",
    "format:diff": "prettier --list-different projects/charts-angular/src/",
    "ci-check": "yarn format:diff"
  },
  "peerDependencies": {
    "@angular/animations": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/common": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/core": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/forms": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/platform-browser": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "rxjs": "^7.8.0",
    "zone.js": "~0.13.0"
  },
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@carbon/charts": "workspace:*",
    "@carbon/icons-angular": "^11.2.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
