    "@types/d3": "^7.4.0",
    "@types/jasmine": "~4.3.1",
    "css-loader": "^6.7.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.5",
    "style-loader": "^3.3.2",
    "typescript": "~4.9.5"
  },
  "prettier": "../../prettier.config.mjs",
  "publishConfig": {
    "directory": "dist",
