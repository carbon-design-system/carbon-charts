{
  "name": "@carbon/charts-react",
  "version": "1.7.6",
  "description": "Carbon charting components for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.min.css": "./dist/styles.min.css",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "postinstall": "carbon-telemetry collect --install",
    "styles": "cp ../core/dist/styles.* dist",
    "license": "cp ../../LICENSE.md ./",
    "build:package": "tsc && vite build && yarn styles && yarn license",
    "mdx": "cp ../core/src/stories/getting-started/react.stories.mdx src/stories && cp ../core/src/stories/tutorials/* src/stories",
    "build:demo": "yarn mdx && NODE_OPTIONS=--max_old_space_size=9126 storybook build -o demo/bundle --quiet",
    "build": "yarn build:package && yarn build:demo",
    "storybook": "storybook dev -p 9006",
    "start": "yarn storybook",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-path ../../.gitignore",
    "format": "prettier . --write \"**/*.{scss,css,js,ts,tsx,cjs,mjs,md}\"  --ignore-path ../../.eslintignore",
    "clean": "rm -rf dist demo/bundle"
  },
  "dependencies": {
    "@carbon/charts": "workspace:*",
    "@carbon/icons-react": "^11.19.0",
    "@carbon/telemetry": "~0.1.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rushstack/eslint-patch": "1.2.0",
    "@stackblitz/sdk": "1.9.0",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/manager-api": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/react-vite": "^7.0.9",
    "@storybook/react-webpack5": "^7.0.9",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.9",
    "@types/carbon__icons-react": "^11.16.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "classnames": "^2.3.2",
    "css-loader": "6.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "storybook": "^7.0.9",
    "style-loader": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "~2.3.0",
    "vite-plugin-inspect": "^0.7.25"
  },
  "prettier": "../../prettier.config.cjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:carbon-design-system/carbon-charts.git",
    "homepage": "https://carbon-design-system.github.io/carbon-charts/react/"
  },
  "license": "Apache-2.0",
  "author": "IBM",
  "keywords": [
    "carbon",
    "charts",
    "react",
    "dataviz",
    "data-visualization",
    "visualizations",
    "d3",
    "svg",
    "component",
    "components",
    "css",
    "html",
    "ibm",
    "typescript",
    "javascript",
    "js",
    "library",
    "pattern",
    "patterns",
    "sass",
    "scss"
  ],
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-charts/issues"
  },
  "homepage": "https://github.com/carbon-design-system/carbon-charts#readme",
  "maintainers": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ],
  "contributors": [
    {
      "name": "Eliad Moosavi",
      "email": "iliadm@ca.ibm.com",
      "url": "https://github.com/theiliad"
    }
  ]
}
