{
	"sourceType": "unambiguous",
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"chrome": 100
				}
			}
		],
		[
			"@babel/preset-typescript",
			{
				"allowDeclareFields": true
			}
		],
		"@babel/preset-react"
	],
	"plugins": ["@babel/plugin-proposal-object-rest-spread", "@babel/plugin-transform-react-jsx"]
}
